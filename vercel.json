{
  "version": 2,
  "buildCommand": "yarn build",
  "devCommand": "yarn dev",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": { "distDir": "dist"}
    }
  ],
  "rewrites": [
  {
   "source": "/(.*)",
   "destination": "dist/src/index.js"
  }
 ]
}
