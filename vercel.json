{
  "version": 2,
  "buildCommand": "yarn build",
  "devCommand": "yarn dev",
  "builds": [
    {
      "src": "dist/src/index.js",
      "use": "@vercel/node",
      "config": { "includeFiles": ["dist/**"]}
    }
  ],
  "rewrites": [
  {
   "source": "/(.*)",
   "destination": "dist/src/index.js"
  }
 ]
}
